# backend/.env.example
# Copy to .env and fill in your values

ENVIRONMENT=development
DEBUG=true

# API
API__NAME=FinancePro
API__VERSION=1.0.0
API__API_VERSION=v1

# Database
DATABASE__URL=postgresql://user:password@localhost:5432/financepro_dev

# Security (CHANGE THIS!)
SECURITY__SECRET_KEY=your-secret-key-here

# CORS
CORS__ALLOWED_ORIGINS=["http://localhost:3000"]

# Features
FEATURE__ENABLE_AI_CLASSIFICATION=true